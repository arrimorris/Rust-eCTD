{
  "productName": "eCTD Forge",
  "version": "0.1.0",
  "identifier": "com.ectd.forge",
  "build": {
    "beforeDevCommand": "npm run dev --prefix ui",
    "beforeBuildCommand": "npm run build --prefix ui",
    "devUrl": "http://localhost:1420",
    "frontendDist": "ui/dist"
  },
  "app": {
    "windows": [
      {
        "title": "eCTD Forge",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png"
    ]
  }
}
